<configuration>
    <appender name="A1" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d [%t] %-5p %c - %m%n</pattern>
        </encoder>
    </appender>
    <logger name="com.amazonaws" level="DEBUG"/>
    <root level="WARN">
        <appender-ref ref="A1"/>
    </root>
</configuration>